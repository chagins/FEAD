  "author": "chagins <chagin_yav@hotmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack serve",
    "dev": "webpack --mode development",
    "build-js": "webpack --mode=production --node-env=production",
    "build-types": "tsc --emitDeclarationOnly",
    "ts-check": "tsc --noEmit",
    "build": "yarn run build-js",
    "test": "jest",
    "watch": "webpack --watch",
    "lint": "yarn run eslint:fix && yarn run prettier:fix",
    "eslint:fix": "eslint --fix --ext js,jsx,ts,tsx src",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,json}\"",
    "netlify-env": "node ./.env.js"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
